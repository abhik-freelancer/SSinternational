@model IEnumerable<SSinternational.viewmodel.EntryBillMasterVM>
@{
    ViewBag.Title = "WMS-Pro";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="container-fluid">
    <div class="header-panel-info">
        <div class="row">
        
            <div class="col-md-6">
                <ul class="header-info-left">
                    <li>List of Entry-Bill</li>
                     
                </ul>
            </div>
            <div class="col-md-6">

              
               
                <ul class="header-info-right">
                    <li>
                        <a href="@Url.Action("addEditBillEntry", "Entrybill")" class="btn btn-primary" role="button">ADD NEW</a>
                    </li>
                    <li><a href="@Url.Action("Index", "Entrybill")" class="btn btn-primary" role="button">LIST</a></li>
                </ul> 
            </div>
       
        </div> 
         </div><!--end row panel-->
    </div>
<div class="body-container">
    <div class="row">
        <div class="col-lg-12">
            <div class="table-responsive">
									<table class="table table-bordered table-hover" id="table-entryBill" > 
										<thead>
												 <tr>
                                                    <th>Bill.</th>
                                                    <th>Date</th>
                                                    <th>Customer</th>
                                                    <th>Garden</th>
                                                    <th>Broker</th>
                                                    <th>Bill Amount</th>
                                                    <th>Action</th>

                                                 </tr>
											</thead>
											<tbody>
                                                @foreach(var Bills in Model){
												<tr>
                                                        <td>@Bills.EntryBillNumber</td>
                                                        <td>@Bills.EntrybillDate.ToString("dd/MM/yyyy")</td>
                                                        <td>@Bills.Customer</td>
                                                        <td>@Bills.gardencode</td>
                                                        <td>@Bills.BrokerName</td>
                                                        <td>@Bills.totalBillAmount</td>
                                                        <td>
                                                            <a href="@Url.Action("addEditBill", "Bill")" class="btn btn-xs btn-primary" role="button">Edit</a>
                                                        </td>
                                                        
                                                </tr>
                                                }
                                            </tbody>
									</table>
            </div>
        </div>

    </div>

</div>

